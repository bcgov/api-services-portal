{
  "name": "bcgov-aps-portal",
  "description": "API Services Portal by BC Gov API Programme Services",
  "private": true,
  "version": "1.1.0",
  "author": "BC Gov APS",
  "repository": "https://github.com/keystonejs/keystone/tree/master/packages/create-keystone-app/example-projects/starter",
  "homepage": "https://github.com/keystonejs/keystone",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/server.js",
  "nodemonConfig": {
    "ignore": [
      "nextapp/*",
      "auth/*",
      "authz/*",
      "test/*",
      "stories/*"
    ],
    "delay": 2500
  },
  "scripts": {
    "prebuild": "tslint -p tsconfig.json --fix",
    "ts-watch": "tsc --watch --jsx react-jsx",
    "ts-build": "tsc",
    "tsoa-build-v1": "tsoa -c tsoa.json spec-and-routes",
    "tsoa-build-v2": "tsoa -c tsoa-v2.json spec-and-routes",
    "tsoa-gen-types": "ts-node tools/tsoaTypes",
    "delete-assets": "ts-node tools/deleteAssets",
    "copy-assets": "ts-node tools/copyAssets",
    "copy-keystone-admin-assets": "ts-node tools/copyKeystoneAdminAssets",
    "x-prestart": "npm run build",
    "x-dev": "nodemon",
    "nextapp-dev": "cross-env NEXT_PUBLIC_MOCKS=on NODE_ENV=development NODE_OPTIONS='--openssl-legacy-provider --no-experimental-fetch --dns-result-order=ipv4first' next dev ./nextapp",
    "batch": "cross-env NODE_ENV=development node dist/server-batch.js",
    "intg-build": "cross-env NODE_ENV=development npm-run-all delete-assets copy-assets ts-build",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='--openssl-legacy-provider --no-experimental-fetch --dns-result-order=ipv4first' npm-run-all delete-assets copy-assets tsoa-gen-types tsoa-build-v1 tsoa-build-v2 ts-build ks-dev",
    "ks-dev": "cross-env NODE_ENV=development DISABLE_LOGGING=true keystone dev --entry=dist/server.js",
    "dev2": "cross-env NODE_ENV=development DISABLE_LOGGING=true keystone --entry=dist/index.js",
    "mock-server": "nodemon ./test/mock-server/server.js",
    "ks-build": "cross-env NODE_ENV=production keystone build --entry=dist/server.js --out=dist2",
    "start": "cross-env NODE_ENV=production keystone start --entry=dist/server.js",
    "build": "NODE_OPTIONS='--dns-result-order=ipv4first' npm-run-all delete-assets copy-assets tsoa-build-v1 tsoa-build-v2 ts-build ks-build copy-keystone-admin-assets",
    "create-tables": "cross-env CREATE_TABLES=true keystone create-tables --entry=dist/server.js",
    "lint": "eslint ./nextapp --ext .ts,.tsx --quiet",
    "lint:ts": "tsc -p ./nextapp/tsconfig.json --noEmit",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.js --coverage --detectOpenHandles",
    "test:next": "jest --config ./nextapp/jest.config.js --coverage",
    "test:next-watch": "jest --watch --config ./nextapp/jest.config.js",
    "test:watch": "jest -w --coverage",
    "test:node": "node_modules/.bin/mocha",
    "generate": "graphql-codegen",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --exit-zero-on-changes",
    "start:coverage": "NODE_OPTIONS='--openssl-legacy-provider --no-experimental-fetch --dns-result-order=ipv4first' nyc npm run start"
  },
  "dependencies": {
    "@bcgov/bc-sans": "^1.0.1",
    "@chakra-ui/react": "^1.6.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@keycloak/keycloak-admin-client": "^17.0.0-dev.26",
    "@keystone-next/admin-ui": "^7.0.0",
    "@keystonejs/access-control": "^7.1.1",
    "@keystonejs/adapter-mongoose": "^11.2.2",
    "@keystonejs/app-admin-ui": "^7.5.2",
    "@keystonejs/app-graphql": "^6.3.2",
    "@keystonejs/app-next": "^5.2.3",
    "@keystonejs/app-static": "^5.1.3",
    "@keystonejs/auth-password": "^5.1.18",
    "@keystonejs/fields": "^21.0.0",
    "@keystonejs/fields-content": "^9.0.4",
    "@keystonejs/fields-markdown": "^5.2.13",
    "@keystonejs/fields-wysiwyg-tinymce": "^5.3.15",
    "@keystonejs/keystone": "^19.3.2",
    "@keystonejs/list-plugins": "^7.1.5",
    "@keystonejs/utils": "^6.1.1",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^9.8.6",
    "connect-mongo": "^3.2.0",
    "connect-redis": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csv-parser": "^3.0.0",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^2.0.0",
    "date-fns": "^2.19.0",
    "dompurify": "^2.4.0",
    "dotenv": "^8.2.0",
    "downshift": "^6.1.0",
    "es6-promise-pool": "^2.5.0",
    "exceljs": "^4.3.0",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "framer-motion": "^4.1.17",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "graphql-tools": "^7.0.2",
    "http-proxy-middleware": "^2.0.6",
    "isomorphic-unfetch": "^3.1.0",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.0.2",
    "just-kebab-case": "^4.2.0",
    "jwks-rsa": "^2.0.5",
    "jwt-decode": "^3.1.2",
    "keycloak-connect": "^17.0.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.0",
    "npmlog": "^6.0.1",
    "numeral": "^2.0.6",
    "openid-client": "^4.2.2",
    "postcss": "^7.0.35",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^3.1.0",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "^8.31.0",
    "react-markdown": "^5.0.3",
    "react-modal": "^3.12.1",
    "react-query": "^3.5.10",
    "react-syntax-highlighter": "^15.4.3",
    "react-uid": "^2.3.1",
    "react-virtuoso": "^1.5.13",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "remark-gfm": "^1.0.0",
    "slugify": "^1.4.6",
    "soap": "^0.43.0",
    "swagger-ui-express": "^4.3.0",
    "swr": "^0.3.9",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "tsc-watch": "^4.5.0",
    "tsoa": "^3.14.1",
    "tsyringe": "^4.5.0",
    "typography": "^0.16.19",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@cypress/code-coverage": "^3.12.39",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-docs": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/react": "^6.4.21",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.2.11",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.24",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.18.63",
    "@types/nodemailer": "^6.4.1",
    "@types/numeral": "^2.0.2",
    "@types/react": "^17.0.0",
    "@types/request": "^2.48.5",
    "@types/sax": "^1.2.1",
    "@types/semver": "^7.3.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-istanbul": "^6.1.1",
    "casual-browserify": "^1.5.19-2",
    "chai": "^4.1.2",
    "chai-http": "^4.2.0",
    "chromatic": "^6.5.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "mocha": "^5.2.0",
    "msw": "^0.28.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "source-map-support": "^0.5.21",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tslint": "^6.1.3",
    "typescript": "4.2.4"
  },
  "nyc": {
    "extends": "./nyc.config.js",
    "all": true,
    "cache": false,
    "sourceMap": false,
    "produceSourceMap": false,
    "report-dir": "coverage",
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "auth/**",
      "authz/**",
      "batch/**",
      "components/**",
      "controllers/**",
      "lists/**",
      "pages/**",
      "services/**"
    ],
    "exclude": [
      "**/*.test.{ts,tsx}",
      "**/__mocks__/**",
      "**/__test__/**",
      "**/test/**"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "parser-plugins": [
      "asyncGenerators",
      "bigInt",
      "classProperties",
      "classPrivateProperties",
      "dynamicImport",
      "importMeta",
      "objectRestSpread",
      "optionalCatchBinding"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
