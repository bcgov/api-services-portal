{
  "name": "bcgov-aps-portal",
  "description": "The APS API Services Portal",
  "private": true,
  "version": "0.1.0",
  "author": "BC Gov APS",
  "repository": "https://github.com/keystonejs/keystone/tree/master/packages/create-keystone-app/example-projects/starter",
  "homepage": "https://github.com/keystonejs/keystone",
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -p tsconfig.json --fix",
    "ts-build": "tsc",
    "tsoa-build": "tsoa spec-and-routes",
    "copy-assets": "ts-node tools/copyAssets",
    "copy-keystone-admin-assets": "ts-node tools/copyKeystoneAdminAssets",
    "x-prestart": "npm run build",
    "x-dev": "nodemon",
    "dev": "cross-env NODE_ENV=development npm-run-all copy-assets tsoa-build ts-build ks-dev",
    "ks-dev": "cross-env NODE_ENV=development DISABLE_LOGGING=true keystone dev --entry=dist/server.js",
    "dev2": "cross-env NODE_ENV=development DISABLE_LOGGING=true keystone --entry=dist/index.js",
    "mock-server": "nodemon ./test/mock-server/server.js",
    "ks-build": "cross-env NODE_ENV=production keystone build --entry=dist/server.js --out=dist2",
    "start": "cross-env NODE_ENV=development keystone start --entry=dist/server.js",
    "build": "npm-run-all copy-assets tsoa-build ts-build ks-build copy-keystone-admin-assets",
    "create-tables": "cross-env CREATE_TABLES=true keystone create-tables --entry=dist/server.js",
    "lint": "eslint .",
    "lint:ts": "eslint ./nextapp --ext .ts,.tsx --quiet & tsc -p ./nextapp/tsconfig.json --noEmit",
    "test": "cross-env NODE_ENV=test jest --config ./jest.config.js --coverage --detectOpenHandles",
    "test:watch": "jest -w --coverage",
    "test:node": "node_modules/.bin/mocha",
    "generate": "graphql-codegen",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
