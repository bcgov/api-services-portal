{
  "name": "bcgov-aps-portal-e2e",
  "version": "1.0.0",
  "description": "End to End and Integration Tests for APS Portal",
  "main": "index.js",
  "author": "BC Gov APS",
  "license": "MIT",
  "scripts": {
    "cy:open": "cypress open --config-file=cypress.config.ts --env TransferProtocol='http'",
    "cy:run": "npx cypress run --headed --config-file cypress.config.ts --env TransferProtocol='http' --browser ${BROWSER:-edge}",
    "cy:run:rcd": "npm run cy:run -- --record",
    "moch:json": "mochawesome-merge -f results/*.json -o results/bcgov-aps-e2e-report.json",
    "cy:chromium": "cypress open --browser /usr/bin/chromium",
    "cy:edge": "npx cypress run --headed --browser edge",
    "moch:html": "marge results/bcgov-aps-e2e-report.json --reportDir results/report -i -t \"API Services Portal E2E Test Report\"",
    "cy:chrome": "cypress run --headed --env TransferProtocol='http' --browser chrome",
    "cy:run:html": "run-s --continue-on-error cy:run moch:json moch:html",
    "cy:run:rcd:html": "run-s --continue-on-error cy:run:rcd moch:json moch:html",
    "cy:chrome:headless": "cypress run --env TransferProtocol='http' --browser chrome --headless",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:firefox:headless": "cypress run --env TransferProtocol='http' --browser firefox --headless",
    "covrpt": "serve -l 3030"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.0",
    "@types/dateformat": "^5.0.0",
    "@types/jsonpath": "^0.2.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mocha": "^8.2.3",
    "@types/mochawesome": "^6.2.0",
    "@types/node": "^16.0.0",
    "@types/request": "^2.48.7",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "cypress": "^13.0.0",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.2.3",
    "cypress-slow-down": "^1.2.1",
    "cypress-v10-preserve-cookie": "^1.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jsonpath": "^1.1.1",
    "prettier": "^2.3.2",
    "serve": "^14.0.0"
  },
  "dependencies": {
    "@atomist/yaml-updater": "^1.0.2",
    "@cypress/instrument-cra": "^1.4.0",
    "cypress-ntlm-auth": "^4.1.2",
    "cypress-xpath": "^1.6.2",
    "dotenv": "^10.0.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "ms": "^2.1.3",
    "njwt": "^1.2.0",
    "node-fetch": "^3.1.1",
    "node-jose": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "request": "^2.88.2",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2",
    "yaml": "^2.1.3",
    "yamljs": "^0.3.0"
  }
}
